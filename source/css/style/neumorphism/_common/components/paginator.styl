  .paginator {
    line-height: 2em
    .page-number {
      user-select: none
      margin-right: 1em
      background: $btn-bg-circle
      color: rgba($color-blue-gray, 0.6)
      display: inline-flex
      justify-content: center
      width: 2em
      height: 2em
      box-sizing: border-box
      border-radius: 1em
      border: 1px solid #F3F7FB
      box-shadow: boxShadow(0.2vw, 'light-out')
      text-shadow: textShadow('light', 0.05vw, rgba(#E1EFF3, 0.6), rgba(#000, 0.8))
      //box-shadow: 5px 5px 22px 0 #bebebe,
      //       -5px -5px 12px 0 #ffffff
      transition: all .3s ease-in-out
      &:active {
        background: $bg-lg-active
        box-shadow: boxShadow(0.2vw, 'light-out'), boxShadow(0.2vw, 'light-inset')
        //box-shadow: 5px 5px 22px 0 #bebebe,
        //     -5px -5px 12px 0 #ffffff,
        //     inset 2px 2px 6px 0 #bebebe,
        //     inset -2px -2px 6px 0 #ffffff
      }
      &.current {
        //background: $color-red
        cursor: default
        color: rgba(#fff, 0.1)
        box-shadow: boxShadow(0.2vw, 'light-out'), boxShadow(0.2vw, 'light-inset')
        //box-shadow: 5px 5px 22px 0 #bebebe,
        //     -5px -5px 12px 0 #ffffff,
        //     inset 2px 2px 6px 0 #bebebe,
        //     inset -2px -2px 6px 0 #ffffff
      }
    }
    .extend {
      user-select: none
      height: 2em
      padding: 0 1em
      border-radius: 0.5em
      display: inline-flex
      color: rgba($color-blue-gray, 0.6)
      box-shadow: boxShadow(0.2vw, 'light-out')
      text-shadow: textShadow('light', 0.05vw, rgba(#E1EFF3, 0.6), rgba(#000, 0.8))
      //box-shadow: 5px 5px 22px 0 #bebebe,
      //       -5px -5px 12px 0 #ffffff
      transition: all .3s ease-in-out
      &:active {
        box-shadow: boxShadow(0.2vw, 'light-out'), boxShadow(0.2vw, 'light-inset')
        //box-shadow: 5px 5px 22px 0 #bebebe,
        //     -5px -5px 12px 0 #ffffff,
        //     inset 2px 2px 6px 0 #bebebe,
        //     inset -2px -2px 6px 0 #ffffff
      }
      &.prev {
        margin-right: 1em
      }
    }
  }